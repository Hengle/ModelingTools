        <td>
          <para styleclass="Table Header"><text styleclass="Table Header" translate="true">Cell (Row, Column)</text></para>
        </td>
        <td>
          <para styleclass="Table Header"><text styleclass="Table Header" translate="true">Concentration stress period 1</text></para>
        </td>
        <td>
          <para styleclass="Table Header"><text styleclass="Table Header" translate="true">Concentration stress period 2</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Table Header"><text styleclass="Table Header" translate="true">8,5</text></para>
